{
  "watch": [
    "."
  ],
  "ext": "js,json",
  "ignore": [
    "node_modules/**/node_modules",
    "*.test.js",
    "*.spec.js",
    "logs",
    "*.log",
    "npm-debug.log*",
    "yarn-debug.log*",
    "yarn-error.log*",
    "lerna-debug.log*",
    ".git",
    ".DS_Store",
    "coverage",
    ".nyc_output",
    ".env.local",
    ".env.development.local",
    ".env.test.local",
    ".env.production.local",
    "**/sys/**",
    "**/proc/**",
    "**/dev/**",
    "**/tmp/**",
    "**/var/**",
    "**/etc/**",
    "**/usr/**",
    "**/bin/**",
    "**/sbin/**",
    "**/lib/**",
    "**/lib64/**",
    "**/boot/**",
    "**/media/**",
    "**/mnt/**",
    "**/opt/**",
    "**/srv/**",
    "**/run/**",
    "**/lost+found/**",
    "**/root/**",
    "**/home/**",
    "**/sbin/**",
    "**/bin/**",
    "**/usr/**",
    "**/lib/**",
    "**/lib64/**",
    "**/etc/**",
    "**/var/**",
    "**/tmp/**",
    "**/dev/**",
    "**/proc/**",
    "**/sys/**"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "delay": "2000",
  "verbose": false,
  "legacyWatch": true,
  "polling": false
} 